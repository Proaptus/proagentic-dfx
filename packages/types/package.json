{
  "name": "@proagentic/types",
  "version": "0.1.0",
  "private": true,
  "description": "Shared TypeScript types for ProAgentic design platform",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./domain": {
      "types": "./src/domain.ts",
      "default": "./src/domain.ts"
    },
    "./api": {
      "types": "./src/api.ts",
      "default": "./src/api.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@proagentic/typescript-config": "*",
    "typescript": "^5.7.2"
  }
}
