  "technical_architecture": {
    "overview": "Multi-agent pipeline transforming natural language requirements into production-ready mechanical component designs with analysis and documentation.",
    
    "technology_stack": {
      "orchestration": {
        "component": "MAS Hub",
        "status": "Existing ProAgentic component",
        "purpose": "Agent discovery, registration, message routing"
      },
      "llm_integration": {
        "primary": "Claude API",
        "fallback": "GPT-4",
        "purpose": "Requirements parsing, concept generation, explanation generation"
      },
      "standards_injection": {
        "component": "SPR Module",
        "status": "Existing - needs extension for engineering standards",
        "purpose": "Inject ISO, DNV, EN standards into agent context"
      },
      "parametric_cad": {
        "component": "CadQuery",
        "version": "2.4+",
        "language": "Python",
        "output_formats": ["STEP", "IGES", "STL", "glTF"],
        "purpose": "Programmatic CAD generation"
      },
      "meshing": {
        "component": "Gmsh",
        "interface": "Python API",
        "purpose": "Generate FEA meshes from geometry"
      },
      "fea_solver": {
        "component": "CalculiX (CCX)",
        "status": "Open source, industry-proven",
        "capabilities": ["Static", "Modal", "Buckling"],
        "purpose": "Structural analysis"
      },
      "post_processing": {
        "component": "PyVista / VTK",
        "purpose": "Visualisation, result extraction"
      },
      "optimisation": {
        "component": "SciPy / Optuna",
        "purpose": "Parameter optimisation, trade-off analysis"
      },
      "report_generation": {
        "component": "ReportLab / WeasyPrint",
        "output_formats": ["PDF", "HTML"],
        "purpose": "Generate documentation"
      },
      "frontend": {
        "framework": "React + TypeScript",
        "cad_viewer": "Three.js via @react-three/fiber",
        "fea_viewer": "vtk.js or PyVista HTML export"
      }
    },
    
    "agent_pipeline": [
      {
        "order": 1,
        "agent_id": "REQ-AGENT",
        "name": "Requirements Engineering Agent",
        "inputs": [
          "Natural language requirements",
          "Reference to similar parts (optional)"
        ],
        "outputs": [
          "Structured requirements specification (JSON)"
        ],
        "responsibilities": [
          "Parse functional requirements (loads, environment, life)",
          "Extract interface constraints (mounting, mating parts)",
          "Identify applicable standards",
          "Flag ambiguities, request clarification",
          "Validate completeness"
        ],
        "tools_used": ["LLM", "SPR Module"],
        "priority": "P1"
      },
      {
        "order": 2,
        "agent_id": "CONCEPT-AGENT",
        "name": "Concept Generation Agent",
        "inputs": [
          "Structured requirements"
        ],
        "outputs": [
          "Candidate concepts (2-5 options)",
          "Concept rationale"
        ],
        "responsibilities": [
          "Select appropriate design patterns for part type",
          "Generate candidate topologies",
          "Initial material selection",
          "Rough sizing based on engineering heuristics"
        ],
        "tools_used": ["LLM", "Material Database", "Design Pattern Library"],
        "priority": "P1"
      },
      {
        "order": 3,
        "agent_id": "GEOM-AGENT",
        "name": "Parametric Geometry Agent",
        "inputs": [
          "Candidate concepts",
          "Constraints",
          "Material selection"
        ],
        "outputs": [
          "STEP files",
          "glTF files (for viewing)",
          "Parametric model definitions"
        ],
        "responsibilities": [
          "Convert concepts to parametric CAD models",
          "Apply GD&T and tolerancing rules",
          "Generate variations for trade studies"
        ],
        "tools_used": ["CadQuery", "LLM for code generation"],
        "priority": "P1"
      },
      {
        "order": 4,
        "agent_id": "FEA-AGENT",
        "name": "Analysis Agent",
        "inputs": [
          "CAD geometry (STEP)",
          "Load cases",
          "Material properties",
          "Boundary conditions"
        ],
        "outputs": [
          "FEA results (stress, displacement)",
          "Pass/fail against requirements",
          "Visualisation files"
        ],
        "responsibilities": [
          "Mesh generation (Gmsh)",
          "FEA solve (CalculiX)",
          "Static stress analysis",
          "Deflection/stiffness check",
          "Modal analysis (if required)",
          "Compare against allowables"
        ],
        "tools_used": ["Gmsh", "CalculiX", "PyVista"],
        "priority": "P1"
      },
      {
        "order": 5,
        "agent_id": "DFM-AGENT",
        "name": "Manufacturing Assessment Agent",
        "inputs": [
          "CAD geometry",
          "Target manufacturing process"
        ],
        "outputs": [
          "DfM report",
          "Manufacturability score",
          "Suggested modifications"
        ],
        "responsibilities": [
          "Evaluate DfM for target process",
          "Check tool access, setups, tolerances",
          "Flag difficult/expensive features",
          "Suggest design modifications"
        ],
        "tools_used": ["LLM", "DfM Rule Engine"],
        "priority": "P1"
      },
      {
        "order": 6,
        "agent_id": "COST-AGENT",
        "name": "Cost Estimation Agent",
        "inputs": [
          "CAD geometry",
          "Material",
          "Manufacturing process",
          "Quantity"
        ],
        "outputs": [
          "Cost breakdown",
          "Cost drivers identified"
        ],
        "responsibilities": [
          "Material cost calculation",
          "Manufacturing cost (parametric models)",
          "Tooling cost (if applicable)",
          "Assembly/finishing cost"
        ],
        "tools_used": ["Cost Model Database", "LLM"],
        "priority": "P2"
      },
      {
        "order": 7,
        "agent_id": "OPT-AGENT",
        "name": "Optimisation Agent",
        "inputs": [
          "All candidate designs with results"
        ],
        "outputs": [
          "Ranked options",
          "Pareto frontier",
          "Trade-off analysis"
        ],
        "responsibilities": [
          "Compare candidates against requirements",
          "Run parameter sweeps",
          "Multi-objective optimisation",
          "Generate recommendations"
        ],
        "tools_used": ["SciPy", "Optuna"],
        "priority": "P2"
      },
      {
        "order": 8,
        "agent_id": "DOC-AGENT",
        "name": "Documentation Agent",
        "inputs": [
          "All design data and decisions"
        ],
        "outputs": [
          "Design rationale PDF",
          "Analysis report PDF",
          "Trade-off matrix",
          "Standards compliance checklist"
        ],
        "responsibilities": [
          "Compile design rationale",
          "Generate analysis summary",
          "Create compliance checklist",
          "Produce manufacturing notes"
        ],
        "tools_used": ["ReportLab", "LLM"],
        "priority": "P1"
      }
    ],
    
    "data_schemas": {
      "requirements_input": {
        "description": "Structured requirements extracted from natural language",
        "schema": {
          "part_type": "string",
          "functional": {
            "loads": {
              "static": [{"direction": "string", "magnitude_N": "number"}],
              "dynamic": [{"type": "string", "frequency_hz": "number", "magnitude_g": "number"}],
              "cyclic": {"count": "number", "load_range_N": "number"}
            },
            "environment": {
              "temperature_range_c": {"min": "number", "max": "number"},
              "corrosivity_class": "string",
              "humidity": "string"
            },
            "design_life_years": "number"
          },
          "interface": {
            "mounting": {
              "type": "string",
              "hole_pattern": "string",
              "bolt_sizes": ["string"]
            },
            "envelope": {
              "max_dimensions_mm": {"x": "number", "y": "number", "z": "number"},
              "keep_out_zones": ["object"]
            }
          },
          "performance": {
            "weight": {"target_kg": "number", "max_kg": "number"},
            "stiffness": {"min_frequency_hz": "number", "max_deflection_mm": "number"},
            "cost": {"target_gbp": "number", "max_gbp": "number"},
            "quantity": "number"
          },
          "standards": ["string"],
          "preferences": {
            "material_preference": "string",
            "process_preference": "string"
          }
        }
      },
      
      "material_database_entry": {
        "material_id": "string",
        "name": "string",
        "category": "string",
        "grade": "string",
        "properties": {
          "density_kg_m3": "number",
          "youngs_modulus_gpa": "number",
          "poissons_ratio": "number",
          "yield_strength_mpa": "number",
          "ultimate_strength_mpa": "number",
          "fatigue_limit_mpa": "number",
          "thermal_expansion_per_k": "number"
        },
        "cost_per_kg_gbp": "number",
        "environment_compatibility": ["string"],
        "manufacturing_compatibility": ["string"]
      },
      
      "analysis_result": {
        "geometry_id": "string",
        "analysis_type": "string",
        "solver": "string",
        "mesh_info": {
          "nodes": "number",
          "elements": "number",
          "element_type": "string"
        },
        "results": {
          "max_von_mises_mpa": "number",
          "max_displacement_mm": "number",
          "natural_frequencies_hz": ["number"],
          "safety_factor": "number"
        },
        "pass_fail": {
          "stress": "boolean",
          "deflection": "boolean",
          "frequency": "boolean"
        },
        "output_files": {
          "vtk": "string",
          "png": "string"
        }
      }
    },
    
    "frontend_design": {
      "framework": "React + TypeScript",
      "key_components": [
        {
          "component": "RequirementsInput",
          "purpose": "Capture requirements via form or natural language",
          "features": ["NL parsing", "form validation", "requirement preview"]
        },
        {
          "component": "CADViewer",
          "purpose": "Interactive 3D view of generated geometry",
          "library": "Three.js via @react-three/fiber",
          "features": ["rotate", "zoom", "pan", "section view", "measurements"]
        },
        {
          "component": "FEAViewer",
          "purpose": "Display analysis results on geometry",
          "library": "vtk.js",
          "features": ["stress contours", "displacement", "legend", "probe values"]
        },
        {
          "component": "DesignComparison",
          "purpose": "Compare multiple design options",
          "features": ["side-by-side view", "trade-off table", "Pareto chart"]
        },
        {
          "component": "DownloadPanel",
          "purpose": "Export all outputs",
          "formats": ["STEP", "PDF reports", "spreadsheets"]
        },
        {
          "component": "IterationPanel",
          "purpose": "Request design changes",
          "features": ["modification input", "regenerate trigger", "history"]
        }
      ]
    }
  },

