  "project_identity": {
    "codename": "DFX-AGENTS",
    "project_code": "PROAPTUS-DFX-2025",
    "full_name": "DFX-Agents: Agentic Design for X System",
    "competition_challenge": "Advanced Manufacturing: Detailed Design for X Agents"
  },

  "executive_summary": {
    "one_liner": "An agentic AI system that transforms natural language requirements into production-ready mechanical component designs with full engineering analysis and documentation.",
    "problem": "Engineers spend hours to days designing 'standard boring but critical' mechanical parts. The knowledge exists in textbooks, standards, and heuristics, but applying it is tedious and repetitive.",
    "solution": "Multi-agent pipeline that automates requirements parsing, concept generation, parametric CAD, FEA analysis, manufacturing assessment, cost estimation, and documentation.",
    "differentiation": "Real FEA integration (not just geometry), founder's engineering domain expertise (CAD training, manufacturing patents, satellite/subsea experience), ProAgentic 3x development speed.",
    "target_outcome": "Input requirements in natural language, receive production-ready STEP files, analysis reports, and design rationale in minutes instead of hours."
  },

  "project_charter": {
    "vision": "Compress mechanical component design workflow from hours/days to minutes, outputting production-ready CAD, analysis reports, and design rationale.",
    
    "mission": "Build a multi-agent system that takes natural language requirements for standard mechanical components and automatically generates optimised designs with full engineering justification.",
    
    "objectives": [
      {
        "id": "OBJ-1",
        "description": "Generate valid production-ready CAD files from natural language",
        "success_measure": "STEP files open correctly in standard CAD viewers"
      },
      {
        "id": "OBJ-2",
        "description": "Perform automated structural analysis",
        "success_measure": "FEA results within 10% of manual analysis for benchmark cases"
      },
      {
        "id": "OBJ-3",
        "description": "Apply Design for X principles",
        "success_measure": "System considers manufacturability, cost, assembly in decisions"
      },
      {
        "id": "OBJ-4",
        "description": "Provide explainable design rationale",
        "success_measure": "Every decision logged with justification and standards citations"
      },
      {
        "id": "OBJ-5",
        "description": "Support iterative refinement",
        "success_measure": "User can request changes, system regenerates automatically"
      },
      {
        "id": "OBJ-6",
        "description": "Deliver compelling competition demo",
        "success_measure": "End-to-end demo runs in under 5 minutes"
      }
    ],
    
    "key_differentiators": [
      {
        "differentiator": "Engineering domain expertise",
        "detail": "Founder has CAD training, mechanical engineering degree, manufacturing patents, satellite/subsea/laser machining experience"
      },
      {
        "differentiator": "Real FEA integration",
        "detail": "Not just geometry generation - actual structural analysis with CalculiX solver"
      },
      {
        "differentiator": "SPR Module for standards",
        "detail": "Existing capability to inject ISO, AS9100, DNV standards into agent context"
      },
      {
        "differentiator": "ProAgentic build velocity",
        "detail": "3x faster development validated across Innovate UK studies"
      }
    ],
    
    "constraints": [
      "3-month development window (Dec 2025 - Feb 2026)",
      "Must demonstrate live at interview (no pre-recorded videos)",
      "Must be built on proprietary IP-owned codebase",
      "All development within UK"
    ],
    
    "assumptions": [
      "Open source FEA tools (CalculiX, Gmsh) sufficient for demo-level analysis",
      "CadQuery can generate sufficiently complex parametric geometry",
      "Web-based 3D viewing (Three.js) provides adequate CAD visualisation",
      "LLMs have sufficient knowledge of standard mechanical component patterns"
    ],
    
    "risks": [
      {
        "id": "RISK-1",
        "risk": "FEA integration complexity",
        "likelihood": "Medium",
        "impact": "High",
        "mitigation": "Start FEA integration early, have fallback to hand-calc validation"
      },
      {
        "id": "RISK-2",
        "risk": "Geometry generation failures",
        "likelihood": "Medium",
        "impact": "High",
        "mitigation": "Constrain to well-defined parametric patterns, extensive test cases"
      },
      {
        "id": "RISK-3",
        "risk": "Demo reliability on interview day",
        "likelihood": "Low",
        "impact": "Critical",
        "mitigation": "Offline-capable demo, pre-loaded test cases, redundant hardware"
      },
      {
        "id": "RISK-4",
        "risk": "Scope creep beyond 3 months",
        "likelihood": "High",
        "impact": "Medium",
        "mitigation": "Strict scope boundaries, MVP-first approach"
      }
    ]
  },

